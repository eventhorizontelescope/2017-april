#!/usr/bin/env bash

mkdir tests temp log
for d in {3597..3601}; do
	mkdir -p $d
	/usr/bin/time -v                    \
	        hops2uvfits.py              \
	        --clean --uv --flip_SPT_pol \
	        --outdir $d                 \
	        /data/$d                    \
		>  log/uvfits-$d.log        \
		2> log/uvfits-$d.err        &
done
wait $(jobs -p)
